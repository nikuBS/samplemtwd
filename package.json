{
  "name": "mobile_tworld",
  "version": "0.0.1",
  "description": "sktechx",
  "scripts": {
    "start": "npm run client-dev | npm run server-dev",
    "client-dev": "gulp",
    "client-deploy": "gulp build",
    "client-lint": "jshint src/client",
    "server-dev": "nodemon -e ts -w ./src/server -x npm run server-watch:dev",
    "server-watch:dev": "node -r ts-node/register --inspect=3000 ./src/server/bin/www.ts",
    "server-docker": "npm run client-deploy && pm2-docker start pm2.config.js",
    "server-lint": "tslint -p tslint.json"
  },
  "author": "SKTechx@2018",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "https": "^1.0.0",
    "jquery": "1.12.4",
    "jquery-ui-dist": "^1.12.1",
    "jquery-ui-touch-punch": "^0.2.3",
    "moment": "^2.22.2",
    "path": "^0.12.7",
    "rxjs": "^5.5.11",
    "slick-carousel": "^1.8.1",
    "underscore": "^1.9.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/node": "^9.6.21",
    "connect-redis": "^3.3.3",
    "express-session": "^1.15.6",
    "express-useragent": "^1.0.12",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-css": "^0.1.0",
    "gulp-livereload": "^3.8.1",
    "gulp-rename": "^1.3.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-webserver": "^0.9.1",
    "http-proxy-middleware": "^0.18.0",
    "jshint": "^2.9.5",
    "nodemon": "^1.17.5",
    "pm2": "^2.10.4",
    "redis": "^2.8.0",
    "ts-node": "^6.0.5",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.1"
  }
}
