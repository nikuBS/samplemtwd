<div class="popup-page tw-popup pt0 fixed-bottom">
    <div class="container-wrap">
        <div class="container">
            <div class="complete-wrap">
                <div class="title-box">
                    <i class="icon-complete-120"><span class="blind">complete</span></i>
                    <p class="tx-tit">
                        {{#if data.prodNm}}
                            {{data.prodNm}}{{#if data.isBasFeeInfo}}({{data.basFeeInfo}}){{/if}}<br>
                        {{/if}}
                        <span class="tx-highlight pt10">{{data.prodCtgNm}} {{data.typeNm}} 완료</span>
                    </p>
                </div>
                {{#if data.mytPage}}
                    <div class="tx-contents">
                        <ul class="bt-slice item-one">
                            <li><a href="/myt-join/{{data.mytPage}}" class="btn-round2 btn-style3">나의 가입정보 확인하기 &gt;</a></li>
                        </ul>
                    </div>
                {{/if}}
            </div>
        </div>
    </div>

    <div class="bt-fixed-area">
        <ul class="bt-slice full">
            <li><button type="button" class="btn-floating btn-style2 tw-popup-closeBtn">확인</button></li>
        </ul>
    </div>
</div>
