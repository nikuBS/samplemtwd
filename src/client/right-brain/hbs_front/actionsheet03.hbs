<div class="popup tw-popup">
    <div class="popup-page nobg layer actionsheet has-floating"><!-- actionsheet, has-floating, has-header // -->
        <!-- .container-wrap -->
        <div class="container-wrap">
            <div class="container">
                {{#each data}}
                <!-- .cont-box -->
                <div class="cont-box nogaps noborder">
                    <!-- .cont-actionsheet -->
                    <div class="cont-actionsheet">
                        <ul class="ac-list ac3">
                            {{#each list}}
                                <li class="type2">
                                    <button {{{attr}}}>
                                        {{#if simbol}}
                                        <span class="circle">
                                            <span class="simbol {{{simbol}}}"></span>
                                        </span>
                                        {{/if}}
                                        {{#if img}}
                                        <span class="circle">
                                            <img src="{{img.src}}" alt="{{img.alt}}" width="100%" height="100%" />
                                        </span>
                                        {{/if}}
                                        <span class="txt">{{{value}}}</span>
                                    </button>
                                </li>
                            {{/each}}
                        </ul>
                        {{#btn-floating}}
                        <button {{{attr}}} class="btn-floating btn-style3 fe-close">{{txt}}</button>
                        {{/btn-floating}}
                    </div>
                    <!-- // .cont-actionsheet -->
                </div>
                <!--// .cont-box -->
                {{/each}}
            </div>
        </div>
        <!--// .container-wrap -->
    </div>
    <div class="popup-blind"></div>
</div>
