<!-- .popup hbs -->
<div class="popup-page tw-popup fixed-bottom h60"><!-- 하단에 fixed 버튼으로 여백이 필요할때 .fixed-bottom 기본60여백 / 높이 30단위 class 지원 .h30, .h60, .h90 ... -->
    <!-- #header -->
    <div class="header-wrap">
        <div class="page-header">
            <div class="header-ti">
                <h1>과납 내역 환불 받기</h1>
            </div>
        </div>
    </div>
    <!--// #header -->
    <!-- .container-wrap -->
    <div class="container-wrap">
        <h2 class="blind">내용</h2>
        <div class="container">
            <div class="data-select-wrap">
                <span class="data-tit">과납 내역</span>
            </div>
            <!-- .cont-box -->
            <div class="cont-box nogaps">
                <ul class="list-comp-lineinfo type2 no-bt">
                    {{#each overPayList}}
                    <li>
                        <div class="lineinfo-user">
                            <span class="info-title">과납금액</span>
                            <span class="info-sub">{{dataDt}}</span>
                        </div>
                        <div class="lineinfo-data">
                            <span class="info-title"><span class="num ff-tg-black">{{dataAmt}}</span><span class="ff-tg-medium">원</span></span>
                        </div>
                    </li>
                    {{/each}}
                    <li>
                        <div class="lineinfo-user">
                            <span class="info-title tx-bold">환불 총 금액</span>
                        </div>
                        <div class="lineinfo-data">
                            <span class="info-title point-color"><span class="num ff-tg-black">{{totalOverAmt}}</span><span class="ff-tg-medium">원</span></span>
                        </div>
                    </li>
                </ul>
            </div>
            <!--// .cont-box -->
            <div class="data-select-wrap">
                <span class="data-tit">환불 계좌 입력</span>
            </div>
            <!-- .cont-box -->
            <div class="cont-box nogaps-top">
                <!-- 2018-09-18 info-wrap 삭제 -->
                <h3 class="tabcon-title0 pt20">은행명</h3>
                <div class="inp-tx-wrap">
                    <button class="bt-dropdown big" title="은행선택">은행선택<span class="ico"></span></button>
                    <div class="inputbox bt-add mt6">
                        <span class="input">
                            <input type="number" placeholder="계좌번호(숫자만 입력)" title="계좌번호(숫자만 입력)" aria-labelledby="aria-exp-label1" aria-describedby="aria-exp-desc1" id="fe-bank-account">
                            <button class="cancel" style="display: none;"><span class="blind">삭제</span></button>
                        </span>
                    </div>
                </div>
            </div>
            <!--// .cont-box -->
            <!-- .bt-fixed-area 바닥고정 버튼은 이 안에다가 추가 -->
            <div class="bt-fixed-area">
                <ul class="bt-slice full">
                    <li class="bt-red1">
                        <button>환불 요청</button>
                    </li>
                </ul>
            </div>
            <!-- // .bt-fixed-area 바닥고정 버튼은 이 안에다가 추가 -->
        </div>
    </div>
    <!-- // .container-wrap -->
    <!-- .popup-closeBtn -->
    <button class="prev-step tw-popup-closeBtn fe-common-back">취소</button>
    <!--// .popup-closeBtn -->
</div>
<!-- // .popup hbs -->
