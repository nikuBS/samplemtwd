<div class="popup tw-popup">
    <div class="popup-page nobg layer actionsheet {{#if btnfloating}}has-floating{{/if}}"><!-- actionsheet, has-floating, has-header // -->
        <!-- .container-wrap -->
        <div class="container-wrap">
            <div class="container">
                <!-- .cont-box -->
                <div class="cont-box nogaps noborder">
                    <!-- .cont-actionsheet -->
                    <div class="cont-actionsheet">
                        {{#each data}}
                            {{#if @index}}
                                <hr aria-hidden="true"/>
                            {{/if}}
                            {{#if title}}<div class="ac-tit">{{title}}</div>{{/if}}
                            <ul class="ac-list">
                                {{#each list}}
                                    <li class="type1">
                                        {{#if button-attr}}<button {{{button-attr}}}>{{/if}}
                                        {{#if a-attr}}<a {{{a-attr}}}>{{/if}}
                                        {{#if label-attr}}<label {{{label-attr}}}>{{/if}}
                                            <span class="table">
                                                <span class="table-row">
                                                    <span class="table-cell">
                                                        {{#if icon}}<span class="simbol {{icon}}"></span>{{/if}}
                                                        {{#if txt}}<span class="txt">{{txt}}</span>{{/if}}
                                                        {{#if add}}<span class="add">{{add}}</span>{{/if}}
                                                    </span>
                                                    <span class="table-cell right">
                                                        {{#if spot}}<span class="spot">{{spot}}</span>{{/if}}
                                                        {{#if radio-attr}}<span class="custom-form"><input type="radio" {{{radio-attr}}}><span class="r-type1"></span></span>{{/if}}
                                                        {{#if checkbox-attr}}<span class="custom-form"><input type="checkbox" {{{checkbox-attr}}}><span class="c-type1"></span></span>{{/if}}
                                                        <span class="link"></span>
                                                    </span>
                                                </span>
                                            </span>
                                        {{#if label-attr}}</label>{{/if}}
                                        {{#if a-attr}}</a>{{/if}}
                                        {{#if button-attr}}</button>{{/if}}
                                    </li>
                                {{/each}}
                            </ul>
                        {{/each}}
                        {{#if lineManagement}}
                        {{#lineManagement}}
                            <hr aria-hidden="true"/>
                            <ul class="ac-list">
                                <li class="type1">
                                    <button {{{attr}}}>
                                        <span class="table">
                                            <span class="table-row">
                                                <span class="table-cell">
                                                    <span class="txt">{{{txt}}}</span>
                                                </span>
                                                <span class="table-cell right">
                                                    <span class="link"></span>
                                                </span>
                                            </span>
                                        </span>
                                    </button>
                                </li>
                            </ul>
                        {{/lineManagement}}
                        {{/if}}
                        {{#if btnfloating}}
                        {{#btnfloating}}
                            <button {{{attr}}} class="btn-floating btn-style3">{{{txt}}}</button>
                        {{/btnfloating}}
                        {{/if}}
                    </div>
                    <!-- // .cont-actionsheet -->
                </div>
                <!--// .cont-box -->
            </div>
        </div>
        <!--// .container-wrap -->
    </div>
    <div class="popup-blind"></div>
</div>
