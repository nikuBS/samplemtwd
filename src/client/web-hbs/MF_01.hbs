<div class="popup tw-popup">
    <div class="popup-page nobg layer actionsheet has-floating"><!-- actionsheet, has-floating, has-header // -->
        <!-- .container-wrap -->
        <div class="container-wrap">
            <div class="container">
                <!-- .cont-box -->
                <div class="cont-box nogaps noborder">
                    <!-- .cont-actionsheet -->
                    <div class="cont-actionsheet mf-actionsheet">
                        {{#if lineManagement}}
                            {{#lineManagement}}
                                <div class="lineManagement-header">
                                    <ul class="ac-list">
                                        <li class="type1">
                                            <button {{{attr}}}>
                                                <span class="table">
                                                    <span class="table-row">
                                                        <span class="table-cell">
                                                            <span class="txt">{{{txt}}}</span>
                                                        </span>
                                                        <span class="table-cell right">
                                                            {{#if spot}}<span class="spot">{{spot}}</span>{{/if}}
                                                            <span class="link"></span>
                                                        </span>
                                                    </span>
                                                </span>
                                            </button>
                                        </li>
                                    </ul>
                                    <hr aria-hidden="true"/>
                                </div>
                            {{/lineManagement}}
                        {{/if}}
                        {{#each data}}
                            {{#if @index}}
                                <hr aria-hidden="true"/>
                            {{/if}}
                            {{#if title}}<div class="ac-tit">{{title}}</div>{{/if}}
                            <ul class="ac-list">
                                {{#each list}}
                                     <li class="type3 {{option}}">
                                        <button>
                                            <span class="table">
                                                <span class="table-row">
                                                    <span class="table-cell">
                                                        <span class="txt">{{txt}}{{#if add}}<span class="add">{{add}}</span>{{/if}}</span>
                                                    </span>
                                                    <span class="table-cell right">
                                                        <span class="spot">{{{spot}}}</span>
                                                        <span class="link"></span>
                                                    </span>
                                                </span>
                                            </span>
                                        </button>
                                    </li>
                                {{/each}}
                            </ul>
                        {{/each}}
                        {{#btnfloating}}
                            <button type="button" {{{attr}}} class="btn-floating btn-style3 tw-popup-closeBtn">{{{txt}}}</button>
                        {{/btnfloating}}
                    </div>
                    <!-- // .cont-actionsheet -->
                </div>
                <!--// .cont-box -->
            </div>
        </div>
        <!--// .container-wrap -->
    </div>
    <div class="popup-blind"></div>
</div>
