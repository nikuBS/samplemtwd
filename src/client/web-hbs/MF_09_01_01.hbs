<!-- .popup hbs -->
<div class="popup-page nobg tw-popup fixed-bottom txt-screed"><!-- .fixed-bottom 하단에 고정된 버튼이 있는 경우 -->
    <!-- #header -->
    <div class="header-wrap">
        <div class="page-header">
            <div class="header-ti">
                <h1>{{data.0.title}}</h1>
            </div>
        </div>
    </div>
    <!--// #header -->
        <!-- .container-wrap -->
        <div class="container-wrap">
            <div class="container">
                <!-- .cont-box -->
                <hr class="cont-sp type1" aria-hidden="true" />
                <div class="cont-box nogaps noborder">
                    <!-- .cont-actionsheet -->
                    <div class="cont-actionsheet mf-actionsheet">
                        {{#each data}}
                            {{#if @index}}
                                <hr aria-hidden="true"/>
                            {{/if}}
                            <ul class="ac-list">
                                {{#each list}}
                                     <li class="type3 {{option}}">
                                        <button>
                                            <span class="table">
                                                <span class="table-row">
                                                    <span class="table-cell">
                                                        <span class="txt">{{txt}}{{#if add}}<span class="add">{{add}}</span>{{/if}}</span>
                                                    </span>
                                                    <span class="table-cell right">
                                                        <span class="spot">{{{spot}}}</span>
                                                        <span class="link"></span>
                                                    </span>
                                                </span>
                                            </span>
                                        </button>
                                    </li>
                                {{/each}}
                            </ul>
                        {{/each}}
                        {{#btnfloating}}
                            <button type="button" {{{attr}}} class="btn-floating btn-style3 tw-popup-closeBtn">{{{txt}}}</button>
                        {{/btnfloating}}
                    </div>
                    <!-- // .cont-actionsheet -->
                </div>
                <!--// .cont-box -->
            </div>
        </div>
        <!--// .container-wrap -->
    <!-- .popup-closeBtn -->
    <button class="popup-closeBtn fe-close-pop"><span class="blind">팝업 페이지 닫기</span></button>
    <!--// .popup-closeBtn -->
</div>
<!--// .popup hbs -->
