<div class="popup-page tw-popup fixed-bottom nobg txt-screed" data-menuId="M001733">
    <!-- .popup-closeBtn -->
    <button class="prev-step fe-btn_cancel_join">취소</button>
    <!--// .popup-closeBtn -->
    <!-- #header -->
    <div class="header-wrap">
        <div class="page-header">
            <div class="header-ti">
                <h1>{{title}}</h1>
            </div>
        </div>
    </div>
    <!--// #header -->
    <!-- .container-wrap -->
    <div class="container-wrap">
        <h2 class="blind">내용</h2>
        <div class="container">
            <!-- .cont-box -->
            <hr class="cont-sp type1" aria-hidden="true"/>
            <div class="cont-box">
                <div class="user-info">
                    <span class="blind" aria-hidden="true">현재 선택된 회선정보 및 사용중인 {{typeText}} 정보 노출</span>
                    <span class="ico"><span class="blind" aria-hidden="true">선택된 회선정보 아이콘</span></span>
                    <div class="in-info">
                        {{#if isMobilePlan}}
                        <span class="number"><span class="blind">회선번호</span>{{svcNumMask}}</span>
                        <span class="sub-info">
                            <span class="elem"><span class="blind">사용요금제명</span>{{svcProdNm}}</span>
                            {{#if svcProdBasFeeInfo}}
                                <span class="elem">
                                <span class="blind">월 이용요금</span>
                                    {{#if svcProdBasFeeInfo.isNaN}}
                                        {{svcProdBasFeeInfo.value}}
                                    {{else}}
                                        {{svcProdBasFeeInfo.value}} 원
									<span class="tx-regular">(월/부가세 포함)</span><!-- 190605  수정_01 -->
                                    {{/if}}
                            </span>
                            {{/if}}
                        </span>
                        {{else}}
                        <span class="number lh-full"><span class="blind">회선번호</span>{{svcNumMask}}</span>
                        {{/if}}
                    </div>
                </div>
            </div>
            <!--// .cont-box -->

            <hr class="cont-sp type1" aria-hidden="true"/>

            <!-- .cont-box -->
            <div class="cont-box">
                <div class="charge-guide-wrap">
                    <div class="charge-cont">
                        <span class="sub-disc01">
                          <span class="elem">{{joinTypeText}}할 {{typeText}}</span>
                        </span>
                        {{#if isMobilePlan}}
                            <span class="tit-tx"><span class="blind">변경 요금제명</span>{{toProdName}}</span>
                        {{else}}
                            <span class="tit-tx"><span class="blind">{{joinTypeText}}할 상품정보</span>{{toProdName}}</span>
                        {{/if}}
                        {{#if isNumberBasFeeInfo}}
							<span class="sub-disc03"><span class="tx-bold vbl">{{toProdBasFeeInfo}}원 </span>(월/부가세 포함)</span>
                        {{else}}
                            <span class="sub-disc03"><span class="tx-bold vbl">{{toProdBasFeeInfo}}</span></span>
                        {{/if}}
                    </div>
                    {{#if toProdDesc}}
                        <div class="cont-noti mt36">
                            <p class="type1">{{{toProdDesc}}}</p>
                        </div>
                    {{/if}}
                    {{#if isComparePlan}}
                        <div class="pt30">
                            <ul class="bt-slice">
                                <li class="bt-blue1">
                                    <button class="fe-btn_compare_plans" aria-haspopup="true">사용 중인 요금제와 비교하기</button>
                                </li>
                            </ul>
                        </div>
                    {{/if}}
                </div>
            </div>
            <!--// .cont-box -->
            {{#if settingSummaryTexts}}
                <hr class="cont-sp type1" aria-hidden="true"/>

                <!-- .cont-box -->
                <div class="cont-box nogaps-hoz">
                    <ul class="link-list set-info {{{setInfo}}}">
                        <li>
                            <dl>
                                <dt>설정 정보</dt>
                                <dd>
                                    {{#settingSummaryTexts}}
                                        <span class="{{spanClass}}">{{text}}</span>
                                    {{/settingSummaryTexts}}
                                    <button class="fe-btn_close_confirm {{{setInfo}}}">정보설정 팝업 열기</button>
                                </dd>
                            </dl>
                        </li>
                    </ul>
                </div>
                <!--// .cont-box -->
            {{/if}}
            <hr class="cont-sp type1" aria-hidden="true"/>

            {{#if isJoinTermProducts}}
                <!-- .cont-box -->
                <div class="cont-box nogaps-top">
                    <div class="info-list">
                        <ul>
                            <li>
                                <div class="tit-area" tabindex="0">
                                    <span class="tit">자동</span>
                                    <span class="sub-tit">가입되는 항목</span>
                                </div>
                                <div class="cont-area">
                                    {{#if autoJoinList}}
                                        {{#autoJoinList}}
                                            <dl>
                                                <dt>{{svcProdNm}}</dt>
                                                {{#each svcProdList}}
                                                    <dd>{{{this}}}</dd>
                                                {{/each}}
                                            </dl>
                                        {{/autoJoinList}}
                                    {{else}}
                                        <dl>
                                            <dt>부가서비스</dt>
                                            <dd class="list-none">
                                                <ul>
                                                    <li>자동 가입 서비스가 없습니다.</li>
                                                </ul>
                                            </dd>
                                        </dl>
                                    {{/if}}
                                    {{#if isMobilePlan}}
                                        <dl>
                                            <dt>혜택</dt>
                                            {{#if autoJoinBenefitList}}
                                                <dd>
                                                    <ul>
                                                        {{{autoJoinBenefitList}}}
                                                    </ul>
                                                </dd>
                                            {{else}}
                                                <dd class="list-none">
                                                    <ul>
                                                        <li>자동 가입 혜택이 없습니다.</li>
                                                    </ul>
                                                </dd>
                                            {{/if}}
                                        </dl>
                                    {{/if}}
                                </div>
                            </li>
                            <li>
                                <div class="tit-area" tabindex="0">
                                    <span class="tit">자동</span>
                                    <span class="sub-tit">해지되는 항목</span>
                                </div>
                                <div class="cont-area">
                                    {{#if autoTermList}}
                                        {{#autoTermList}}
                                            <dl>
                                                <dt>{{svcProdNm}}</dt>
                                                {{#each svcProdList}}
                                                    <dd>{{{this}}}</dd>
                                                {{/each}}
                                            </dl>
                                        {{/autoTermList}}
                                    {{else}}
                                        <dl>
                                            <dt>부가서비스</dt>
                                            <dd class="list-none">
                                                <ul>
                                                    <li>자동 해지 서비스가 없습니다.</li>
                                                </ul>
                                            </dd>
                                        </dl>
                                    {{/if}}
                                    {{#if isMobilePlan}}
                                        <dl>
                                            <dt>혜택</dt>
                                            {{#if autoTermBenefitList}}
                                                <dd>
                                                    <ul>
                                                        {{{autoTermBenefitList}}}
                                                    </ul>
                                                </dd>
                                            {{else}}
                                                <dd class="list-none">
                                                    <ul>
                                                        <li>자동 해지 혜택이 없습니다.</li>
                                                    </ul>
                                                </dd>
                                            {{/if}}
                                        </dl>
                                    {{/if}}
                                    {{#if isSelectedProgram}}
                                        <dl>
                                            <dt>부가서비스</dt>
                                            {{#if isContractPlan}}
                                                <dd>무약정플랜</dd>
                                            {{else}}
                                                <dd class="tx-black3">자동해지 부가서비스가 없습니다.</dd>
                                            {{/if}}
                                        </dl>
                                    {{/if}}
                                </div>
                            </li>
                        </ul>
                    </div>
                    {{#if downgrade}}
                        <div class="cont-noti pt20">
                            {{#if downgrade.isHtml}}
                                {{{downgrade.guidMsgCtt}}}
                            {{else}}
                                <span class="type2">{{downgrade.guidMsgCtt}}</span>
                            {{/if}}
                        </div>
                    {{/if}}
                </div>
                <!--// .cont-box -->
            {{/if}}

            {{#if isMobilePlan}}
                {{#if isOverpayResult}}
                    <div class="ti-caption-gray">
