<div class="popup tw-popup">
    <div class="popup-page nobg layer actionsheet has-floating"><!-- actionsheet, has-floating, has-header // -->
        <!-- .container-wrap -->
        <div class="container-wrap">
            <div class="container">
                <!-- .cont-box -->
                <div class="cont-box nogaps noborder">
                    <!-- .cont-actionsheet -->
                    <div class="cont-actionsheet mf-actionsheet">
                        <div class="lineManagement-header">
                            <ul class="ac-list">
                                <li class="type1">
                                    <button id="fe-bt-line" type="button">
                                    <span class="table">
                                        <span class="table-row">
                                            <span class="table-cell">
                                                <span class="txt">회선관리</span>
                                            </span>
                                            <span class="table-cell right">
                                                <span class="link"></span>
                                            </span>
                                        </span>
                                    </span>
                                    </button>
                                </li>
                            </ul>
                            <hr aria-hidden="true"/>
                        </div>
                        {{#each data}}
                            {{#if @index}}
                                <hr aria-hidden="true"/>
                            {{/if}}
                            <div class="ac-tit fe-title {{showService}}">{{title}} ({{cnt}})</div>
                            <ul class="ac-list fe-service {{showService}}">
                                {{#each list}}
                                    <li class="type1 fe-item {{showLine}}">
                                        <label>
                                            <span class="table">
                                                <span class="table-row">
                                                    <span class="table-cell">
                                                        {{#if icon}}<span class="simbol {{icon}}"></span>{{/if}}
                                                        {{#if txt}}<span class="txt">{{txt}}</span>{{/if}}
                                                        {{#if add}}<span class="add">{{add}}{{#if badge}}<span class="badge">기준</span>{{/if}}</span>{{/if}}

                                                        {{#if pin}}<span class="pin"><span class="blind">기준회선</span></span>{{/if}}
                                                    </span>
                                                    <span class="table-cell right">
                                                        <span class="custom-form"><input class="fe-radio-line" type="radio" name="line{{@index}}" data-svcMgmtNum="{{svcMgmtNum}}" data-mdn="{{add}}" {{option}}><span class="r-type1"></span></span>
                                                        <span class="link"></span>
                                                    </span>
                                                </span>
                                            </span>
                                        </label>
                                    </li>
                                {{/each}}
                            </ul>
                        {{/each}}
                        {{#if btMore}}
                            <div class="btn-more">
                                <button id="fe-bt-more" type="button">더보기</button>
                            </div>
                        {{/if}}
                        <button type="button" class="btn-floating btn-style3 tw-popup-closeBtn">닫기</button>
                    </div>
                    <!-- // .cont-actionsheet -->
                </div>
                <!--// .cont-box -->
            </div>
        </div>
        <!--// .container-wrap -->
    </div>
    <div class="popup-blind"></div>
</div>
