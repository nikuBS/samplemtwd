<!-- .popup hbs -->
<div class="popup-page tw-popup pt0 fixed-bottom"><!-- 하단에 fixed 버튼으로 여백이 필요할때 .fixed-bottom 기본60여백 / 높이 30단위 class 지원 .h30, .h60, .h90 ... -->
    <!-- .container-wrap -->
    <div class="container-wrap">
        <div class="container">
            <div class="complete-wrap">
                <div class="title-box">
                    <i class="icon-complete-120"><span class="blind">complete</span></i>
                    <p class="tx-tit"><span class="tx-highlight pt10">이용에 불편을 드려서 죄송합니다.</span></p>
                </div>
                <div class="tx-contents">
                    {{#if data}}
                        {{#if data.code}}
                            <p class="code-txt">에러 코드 : {{data.code}}</p>
                        {{/if}}
                        {{#if data.msg}}
                            <p class="basic-txt">{{data.msg}}</p>
                        {{/if}}
                    {{/if}}
                    <ul class="bt-slice item-one">
                        <li><button type="button" class="btn-round2 btn-style3 fe-request">문의하기</button></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="bt-fixed-area">
        <ul class="bt-slice full">
            <li><button type="button" class="btn-floating btn-style2 tw-popup-closeBtn">돌아가기</button></li>
        </ul>
    </div>
    <!--// .container-wrap -->
    <!-- .popup-closeBtn -->
    <!-- <button class="popup-closeBtn"><span class="blind">팝업 페이지 닫기</span></button> -->
    <!--// .popup-closeBtn -->
</div>
<!--// .popup hbs -->