<div class="popup tw-popup">
    <div class="popup-page nobg layer actionsheet has-floating"><!-- actionsheet, has-floating, has-header // -->
        <!-- .container-wrap -->
        <div class="container-wrap">
            <div class="container">
                
                <!-- .cont-box -->
                <div class="cont-box nogaps noborder">
                    <!-- .cont-actionsheet -->
                    <div class="cont-actionsheet">

                        {{#each data1}}
                            {{#if @index}}
                                <hr aria-hidden="true"/>
                            {{/if}}
                            {{#if title}}<div class="ac-tit">{{title}}</div>{{/if}}
                            <ul class="ac-list">
                                {{#each list}}
                                    <li class="type1">
                                        <button {{{button-attr}}}>
                                            <span class="table">
                                                <span class="table-row">
                                                    <span class="table-cell">
                                                        <span class="simbol {{icon}}"></span>
                                                        <span class="txt">{{txt}}</span>
                                                        <span class="add">{{add}}</span>
                                                    </span>
                                                    <span class="table-cell right">
                                                        <span class="spot">{{spot}}</span>
                                                        <span class="link"></span>
                                                    </span>
                                                </span>
                                            </span>
                                        </button>
                                    </li>
                                {{/each}}
                            </ul>
                        {{/each}}

                        


                        {{#each data2}}
                            {{#if @index}}
                                <hr aria-hidden="true"/>
                            {{/if}}
                            {{#if title}}<div class="ac-tit">{{title}}</div>{{/if}}
                            <ul class="ac-list">
                                {{#each list}}
                                <li class="type1">
                                    <label {{{label-attr}}}>
                                        <span class="table">
                                            <span class="table-row">
                                                <span class="table-cell">
                                                    <span class="simbol {{{icon}}}"></span>
                                                    <span class="txt">{{txt}}</span>
                                                    <span class="add">{{add}}</span>
                                                </span>
                                                <span class="table-cell right">
                                                    <span class="custom-form"><input type="radio" {{{input-attr}}}><span class="r-type1"></span></span>
                                                </span>
                                            </span>
                                        </span>
                                    </label>
                                </li>
                                {{/each}}
                            </ul>
                        {{/each}}

                        {{#each data3}}
                            <ul class="ac-list ac3">
                                {{#each list}}
                                    <li class="type2">
                                        <button {{{attr}}}>
                                            <span class="circle">
                                                <span class="simbol {{{simbol}}}"></span>
                                            </span>
                                            <span class="txt">{{{value}}}</span>
                                        </button>
                                    </li>
                                {{/each}}
                            </ul>
                        {{/each}}

                        {{#each data4}}
                            {{#if @index}}
                                <hr aria-hidden="true"/>
                            {{/if}}
                            {{#if title}}<div class="ac-tit">{{title}}</div>{{/if}}
                            <ul class="ac-list">
                                {{#each list}}
                                     <li class="type3">
                                        <button>
                                            <span class="table">
                                                <span class="table-row">
                                                    <span class="table-cell">
                                                        <span class="txt">{{txt}}<span class="add">{{add}}</span></span>
                                                    </span>
                                                    <span class="table-cell right">
                                                        <span class="spot">{{{spot}}}</span>
                                                        <span class="link"></span>
                                                    </span>
                                                </span>
                                            </span>
                                        </button>
                                    </li>
                                {{/each}}
                            </ul>
                        {{/each}}



                        {{#btnfloating}}
                            <button {{{attr}}} class="btn-floating btn-style3">{{{txt}}}</button>
                        {{/btnfloating}}
                    </div>
                    <!-- // .cont-actionsheet -->
                </div>
                <!--// .cont-box -->
               
            </div>
        </div>
        <!--// .container-wrap -->
    </div>
    <div class="popup-blind"></div>
</div>
